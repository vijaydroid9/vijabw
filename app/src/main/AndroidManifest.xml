<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="raisewave.a30daysabsworkout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DaysList"
            android:screenOrientation="portrait"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".DayPlan"
            android:screenOrientation="portrait"
            android:parentActivityName=".DaysList">
        </activity>
        <activity android:name=".SitupExercise"
            android:screenOrientation="portrait"
            android:noHistory="true">
        </activity>
        <activity android:name=".CrunchesExercise"
            android:screenOrientation="portrait"
            android:noHistory="true">
        </activity>
        <activity android:name=".LegRaisesExercise"
            android:screenOrientation="portrait"
            android:noHistory="true">
        </activity>
        <activity android:name=".PlankExercise"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".ExerciseInstruction"
            android:screenOrientation="portrait"
            android:noHistory="true">
        </activity>
        <activity android:name=".TotalExerciseList"
            android:screenOrientation="portrait"
            android:noHistory="true">
        </activity>
        <activity android:name=".Progress"
            android:screenOrientation="portrait"
            android:noHistory="true">
        </activity>
        <activity android:name=".Settings"
            android:screenOrientation="portrait">
        </activity>
        <receiver android:name=".NotifyReceiver"
            android:enabled="true">
        </receiver>
    </application>

</manifest>